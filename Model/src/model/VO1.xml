    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT NATIONAL_NO, 
                      EMP_NO                     AS EmpConstantNo, 
                      BRANCH_CD                  AS EmpDepartmentNo, 
                      (COST_CENTER_CD_7)         AS CostOper, 
                      COST_CENTER_CD, 
                      YEART, 
                      EMP_RESPONSIBLE_ID, 
                      job_cd, 
                      job_date, 
                      TRIM (EMP_NAME)            AS empname, 
                      TRIM (JOB_NAME)            AS jobname, 
                      TRIM (QUAL_NAME)           qualname, 
                      QUAL_DATE, 
                      JOIN_DATE                  AS joindate, 
                      BIRTH_DATE                 AS birthdate, 
                      TRIM (LEVEL_NAME)          AS levelname, 
                      LEVEL_DATE, 
                      TRIM (BRANCH_NAME)         branchname, 
                      TRIM (COST_CENTER_NAME)    AS costcentername, 
                      NO_DAYS, 
                      G_FIXED, 
                      G_VARIABLE, 
                      TOTAL, 
                      SAFI, 
                      CEIL (30 * safi / NO_DAYS) AS c, 
                      SHAMEL_SALARY              AS shamelsalary, 
                      BASIC_SALARY               AS basicsalary 
                 FROM TAXES_FIX_VAR 
                WHERE     yeart = :yearT1 
                      AND EMP_NO = :empNo 
                      AND emp_status_id = 0 
                      AND LEVEL_CD NOT IN (30, 
                                           32, 
                                           40, 
                                           42, 
                                           46)]]></SQLQuery>
