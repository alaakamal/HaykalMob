      Kind="where"
      Type="oracle.jbo.domain.Number"/>
    <SQLQuery><![CDATA[SELECT EMP_CONSTANT_NO,emp_national_no,     
                      EMP_NAME_ARAB,      
                      EMP_DEPARTMENT_NO,      
                      BRANCH_A_NM,      
                      EMP_BIRTH_DATE,      
                      EMP_JOINING_DATE,      
                      EMP_CONTRACT_DATE,      
                      EMP_BASIC_SALARY,      
                      EMP_BASIC_SALARY_DENIED,      
                      TBL_OCCUPATION_NAME,      
                      TBL_LEVEL_NAME,      
                      EMP_LEVEL_DATE,      
                      TBL_QUALIFICATION_NAME,      
                      EMP_QULIFICATION_DATE,      
                      CASE      
                         WHEN EMP_MOVE_MAIN_CODE = 10 THEN TBL_MOVE_SUB_NAME      
                      END      
                         TBL_MOVE_SUB_NAME,      
                      CASE      
                         WHEN EMP_MOVE_MAIN_CODE = 10 THEN EMP_MOVE_SUB_DATE      
                      END      
                         EMP_MOVE_SUB_DATE,      
                      ak_fun (emp_national_no, 1) AS rep1,      
                      ak_fun (emp_national_no, 2) AS year1,      
                      ak_fun (emp_national_no, 3) AS rep2,      
                      ak_fun (emp_national_no, 4) AS year2      
                 FROM FIX_DATA      
                WHERE EMP_CONSTANT_NO = :empNo]]></SQLQuery>
